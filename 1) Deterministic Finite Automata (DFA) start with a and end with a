#include <stdio.h>
#include <string.h>

#define MAX_LEN 100

int main()
{
    char string[MAX_LEN];
    int len;
    int state = 0;
    printf("Enter the string: ");
    scanf("%s", string);
    len = strlen(string);

    for (int i = 0; i < len; i++) {
        if (state == 0 && string[i] == 'a') {
            state = 1;
        } else if (state == 1 && string[i] != 'a') {
            state = 2;
        } else if (state == 2 && string[i] == 'a') {
            state = 3;
        } else {
            break;
        }
    }

    if (state == 3) {
        printf("Accepted\n");
    } else {
        printf("Rejected\n");
    }
    return 0;
}
